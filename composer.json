{
    "name": "saaze/core",
    "description": "The core package for Saaze CMS",
    "license": "MIT",
    "authors": [
        {
            "name": "Gilbert Pellegrom",
            "email": "gilbert@pellegrom.me"
        }
    ],
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Saaze\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Saaze\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.3",
        "adbario/php-dot-notation": "^2.2",
        "erusev/parsedown-extra": "^0.8.1",
        "laravel/lumen-framework": "^8.2",
        "spatie/yaml-front-matter": "^2.0",
        "symfony/console": "^5.1",
        "symfony/finder": "^5.1",
        "symfony/yaml": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "config": {
        "sort-packages": true
    }
}
